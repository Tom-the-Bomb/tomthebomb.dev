{
    "trailingComma": "all",
    "semi": true,
    "bracketSpacing": true,
    "tabWidth": 4,
    "endOfLine": "auto",
    "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ]
}
